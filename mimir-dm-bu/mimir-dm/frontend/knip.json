{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/main.ts",
    "src/chat.ts",
    "src/context-debug.ts",
    "src/reference.ts",
    "src/log-viewer.ts",
    "src/sources-main.ts"
  ],
  "project": ["src/**/*.{ts,vue}"],
  "ignore": [
    "src/**/*.d.ts",
    "src/assets/**"
  ],
  "ignoreDependencies": [
    "@tauri-apps/plugin-dialog",
    "@tauri-apps/plugin-shell",
    "autoprefixer",
    "postcss",
    "tailwindcss"
  ],
  "vue": {
    "entry": ["src/**/*.vue"]
  }
}
